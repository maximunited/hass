custom_header:
  clock_format: 24
  compact_mode: true
  exceptions: []
  header_text: Maxim's Home Assistant
  hide_help: true
  options: clock
  tabs_color:
    '0': green
    '7': '#FFFF00'
    '10': green
    '14': 'rgb(3, 252, 119)'
    '15': red
  voice: overflow
