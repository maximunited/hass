        style: |
          ha-card {
            height: 180px;
            width: 250px;
            border-radius: 8px;
            box-shadow: 2px 2px rgba(0,0,0,0.3);
            #padding: 1%;
            #color: [[ if(person.maxim == "home", "red", "") ]];
